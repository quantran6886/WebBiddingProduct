	<script type="text/javascript" src="~/libclient/http://maps.google.com/maps/api/js?sensor=false"></script>
	<script type="text/javascript" src="~/libclient/rs-plugin/js/jquery.themepunch.tools.min.js"></script>
	<script type="text/javascript" src="~/libclient/rs-plugin/js/jquery.themepunch.revolution.min.js"></script>
	<script type="text/javascript" src="~/libclient/js/custom.js"></script>
	<script>
		function fcLogin(){
			$("#loginModal").css("z-index","99999")
			$("#loginModal").modal("show");
		}

	</script>
	@await RenderSectionAsync("Scripts", required: false)
